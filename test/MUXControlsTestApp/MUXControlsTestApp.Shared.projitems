  <!-- There's no way to have a conditional statement in XAML akin to #ifdef BUILD_WINDOWS, so in order
       to use the correct namespace when building WUXC, we copy all of the XAML files to a temp folder,
       edit their namespaces to be the correct namespaces, and then use those XAML files as our XAML compilation source
       instead of the ones in the project. -->
  <Target Name="EditXamlFiles" BeforeTargets="MarkupCompilePass1" Condition="$(BuildingWithBuildExe) == 'true'">
    <PropertyGroup>
      <EditedXamlFilesDir>$(OutDir)\EditedXamlFiles</EditedXamlFilesDir>
    </PropertyGroup>
    <MakeDir Directories="$(EditedXamlFilesDir)" />
    <CopyWithReplacements SourceFiles="%(Page.Identity)" DestinationFiles="$(EditedXamlFilesDir)\%(Page.Filename)%(Page.Extension)" Patterns="Microsoft.UI.Xaml;Microsoft.UI.Private.Controls;Microsoft.UI.Private.Media" Replacements="Windows.UI.Xaml;Windows.UI.Xaml.Controls;Windows.UI.Xaml.Media" />
    <ItemGroup>
      <EditedPage Include="$(EditedXamlFilesDir)\%(Page.Filename)%(Page.Extension)" />
      <Page Remove="@(Page)" />
      <Page Include="@(EditedPage)" />
    </ItemGroup>
    <CopyWithReplacements SourceFiles="%(ApplicationDefinition.Identity)" DestinationFiles="$(EditedXamlFilesDir)\%(ApplicationDefinition.Filename)%(ApplicationDefinition.Extension)" Patterns="Microsoft.UI.Xaml;Microsoft.UI.Private.Controls;Microsoft.UI.Private.Media;&lt;controls:XamlControlsResources/&gt;" Replacements="Windows.UI.Xaml;Windows.UI.Xaml.Controls;Windows.UI.Xaml.Media;&lt;ResourceDictionary/&gt;" />
    <ItemGroup>
      <EditedApplicationDefinition Include="$(EditedXamlFilesDir)\%(ApplicationDefinition.Filename)%(ApplicationDefinition.Extension)" />
      <ApplicationDefinition Remove="@(ApplicationDefinition)" />
      <ApplicationDefinition Include="@(EditedApplicationDefinition)" />
    </ItemGroup>
    <ItemGroup>
      <FileWrites Include="@(EditedPage)" />
    </ItemGroup>
  </Target>
  <Target Name="CustomAfterGenerateAppPackages" AfterTargets="_GenerateAppxPackage" Condition="$(BuildingWithBuildExe) != 'true'">
    <RunPowershellScript Path="$(MSBuildThisFileDirectory)\..\..\tools\ExtractPackageDependencies.ps1" Parameters="-sourceFile $(OutDir)\$(MSBuildProjectName).build.appxrecipe -platform $(PlatformName) -outputFile $(AppxPackageTestDir)$(AppxPackageName).dependencies.txt" FilesWritten="$(AppxPackageTestDir)$(AppxPackageName).dependencies.txt" />
  </Target>
  <PropertyGroup Condition="'$(IsTDPConfiguration)' == 'true'">
    <AppxPackageName Condition="'$(AppxPackageName)'==''">$(TargetName)</AppxPackageName>
    <AppxPackageExtension Condition="'$(AppxPackageExtension)'==''">.appx</AppxPackageExtension>
    <AppxPackageTestDir Condition="'$(AppxPackageTestDir)'==''">$(AppxPackageDir)$(AppxPackageName)_Test\</AppxPackageTestDir>
    <AppxPackageOutput Condition="'$(AppxPackageOutput)'==''">$(AppxPackageTestDir)$(AppxPackageName)$(AppxPackageExtension)</AppxPackageOutput>
    <AppxIntermediateExtension Condition="'$(AppxIntermediateExtension)'==''">.intermediate</AppxIntermediateExtension>
  </PropertyGroup>
  <!-- For the purposes of code coverage, we need to know where the intermediates directory is,
       since we need to re-pack MUXControlsTestApp.appx for code coverage.
       We can't get it from any environment variable, so let's write it to a text file as a workaround. -->
  <Target Name="AfterBuild" Condition="!Exists('$(OutDir)IntermediateDirectoryLocation.txt') And $(BuildingWithBuildExe) != 'true'">
    <WriteLinesToFile File="$(OutDir)IntermediateDirectoryLocation.txt" Lines="$(IntermediateOutputPath)" Overwrite="true" Encoding="Ascii" />
    <ItemGroup>
      <FileWrites Include="$(OutDir)IntermediateDirectoryLocation.txt" />
    </ItemGroup>
  </Target>
  <Target Name="GenerateAppxSources" AfterTargets="AfterBuild" Inputs="@(Page);@(AppContent)" Outputs="$(OutDir)AppxSources.g.targets" Condition="$(BuildingWithBuildExe) == 'true'">
    <ItemGroup>
      <AppContentRelativeToPath Include="@(AppContent)">
        <RelativePath>$([MSBuild]::MakeRelative('$(MSBuildThisFileDirectory)\', %(Identity)))</RelativePath>
      </AppContentRelativeToPath>
    </ItemGroup>
