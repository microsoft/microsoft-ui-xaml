            <TextBlock Grid.Row="11" Text="Button" Style="{StaticResource SideHeader}"/>
            <Grid Grid.Column="1" Grid.Row="11" Style="{StaticResource LeftColumn}">
                <Button Content="Button" MinWidth="100">
                    <Button.Resources>
                        <ResourceDictionary>
