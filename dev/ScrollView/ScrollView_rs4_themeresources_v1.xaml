<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="ScrollViewScrollBarsSeparatorBackground" Color="{StaticResource SystemChromeMediumColor}" Opacity="0.9" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="Light">
            <SolidColorBrush x:Key="ScrollViewScrollBarsSeparatorBackground" Color="{StaticResource SystemChromeMediumColor}" Opacity="0.9" />
        </ResourceDictionary>
        <ResourceDictionary x:Key="HighContrast">
            <StaticResource x:Key="ScrollViewScrollBarsSeparatorBackground" ResourceKey="SystemControlPageBackgroundChromeLowBrush" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

    <x:String x:Key="ScrollViewScrollBarsSeparatorExpandDelay">00:00:00.4</x:String>
    <x:String x:Key="ScrollViewScrollBarsSeparatorExpandDuration">00:00:00.1</x:String>
    <x:String x:Key="ScrollViewScrollBarsSeparatorContractDelay">00:00:02</x:String>
    <x:String x:Key="ScrollViewScrollBarsSeparatorContractDelayDisabled">00:00:00</x:String>
    <x:String x:Key="ScrollViewScrollBarsSeparatorContractDuration">00:00:00.1</x:String>
    <x:String x:Key="ScrollViewScrollBarsSeparatorDisplayDelayWithoutAnimation">00:00:00.725</x:String>
    <x:String x:Key="ScrollViewScrollBarsSeparatorExpandDelayWithoutAnimation">00:00:00.4</x:String>
    <x:String x:Key="ScrollViewScrollBarsSeparatorContractDelayWithoutAnimation">00:00:02</x:String>
    <x:String x:Key="ScrollViewScrollBarsNoTouchDuration">00:00:00.5</x:String>
</ResourceDictionary>
