        foreach (Type type in GetMetadataTypesFromList(winmdPaths, referenceWinmds, typeHintWinmds))
        {  
            if (GetCustomAttribute(type, "WebHostHiddenAttribute") == null && GetCustomAttribute(type, "ApiContractAttribute") == null)
