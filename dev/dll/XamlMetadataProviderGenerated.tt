                    WriteLine(string.Format("    L\"{0}\",", typeName));
                    WriteLine(string.Format("    []() {{ return winrt::make<PrimitiveXamlType>((PCWSTR)L\"{0}\"); }});", typeName));
                }
                else
                {
                    RegisterXamlType(type, false /* exploreBaseAndProperties */ );
                }
            }
