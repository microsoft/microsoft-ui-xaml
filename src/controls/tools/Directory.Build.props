<!-- We put a version of Directory.Build.props here so projects in this folder ignore the Directory.Build.props a folder up in the directory tree. -->
<Project ToolsVersion="14.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <BaseOutputPath>$(MSBuildThisFileDirectory)..\..\BuildOutput\obj\Tools\$(Configuration)\</BaseOutputPath>
    <OutDir>$(BaseOutputPath)\$(Platform)\</OutDir>
    <OutputPath>$(OutDir)$(MSBuildProjectName)\</OutputPath>
    <BaseIntermediateOutputPath>$(MSBuildThisFileDirectory)..\..\BuildOutput\obj\Tools\$(Configuration)\$(Platform)\$(MSBuildProjectName)\Intermediates\</BaseIntermediateOutputPath>
    <IntDir>$(BaseIntermediateOutputPath)</IntDir>
    <IntermediateOutputPath>$(BaseIntermediateOutputPath)</IntermediateOutputPath>
    <GeneratedFilesDir>$(BaseIntermediateOutputPath)Generated Files\</GeneratedFilesDir>
    <GenerateProjectSpecificOutputFolder>True</GenerateProjectSpecificOutputFolder>
  </PropertyGroup>
</Project>
